<kmodule xmlns="http://www.drools.org/xsd/kmodule">
<kbase name="rules" packages="rules.forward">
    <ksession name="k-session"/>
</kbase>
<kbase name="bwBase" packages="rules.backward">
    <ksession name="bwKsession"/>
</kbase>
<kbase name="cepKbase" eventProcessingMode="stream" packages="rules.cep">
    <ksession name="cepKsession" clockType="pseudo"/>
</kbase>
<kbase name="templateBase" packages="rules.template">
    <ksession name="templateKsession"/>
</kbase>
</kmodule>
